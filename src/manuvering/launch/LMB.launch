<?xml version="1.0" ?>
<launch>

  <!-- OP3 Manager -->
	<include file="$(find op3_manager)/launch/op3_manager.launch" />	
  <node pkg="manuvering" type="MotionDriver_v1.py" name="motion" output="screen"></node>
  <node pkg="manuvering" type="TaskControl_LMB.py" name="task_control" output="screen"></node>
  <node pkg="image_processing" type="deteksi_tiang.py" name="image_tiang" output="screen"></node>
  <node pkg="image_processing" type="deteksi_bola.py" name="image_bola" output="screen"></node>
	<!-- ini param tiang -->       
	<rosparam file="$(find image_processing)/config/hsv_param_tiang.yaml" />
<!-- ini param bola -->       
	<rosparam file="$(find image_processing)/config/hsv_param_bola.yaml" />

  
  <!-- UVC camera -->
  <node pkg="usb_cam" type="usb_cam_node" name="usb_cam_node" output="screen">
    <param name="video_device" type="string" value="/dev/video0" />
    <param name="image_width" type="int" value="1280" />
    <param name="image_height" type="int" value="720" />
    <param name="framerate " type="int" value="30" />
    <param name="brightness" value="8" />
    <param name="camera_frame_id" type="string" value="cam_link" />
    <param name="camera_name" type="string" value="camera" />
  </node>
    <node pkg="image_processing" type="deteksi_base.py" name="image_base" output="screen"></node>

</launch>
 